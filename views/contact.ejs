<%- include("./partials/head.ejs") %>

<html>

  <body id="body" data-theme='dark'>

    <%- include("./partials/navbar.ejs") %>


    <div class="contact-page-wrapper">
        <div class="contrasting-background-contact">
            Â 
        </div>
        
        
        
        <div class="container mb-5 mt-5 ps-0">
            <div class="row">
              <div class="col">
                <h2 class="mb-5">Contact Me</h2>
              </div>
            </div>
          </div>
      
      
          <div id="contact-me-form-wrapper" class="container mt-5">
              <div class="row">
                  <div class="col col-6 position-relative">
                     <form class="position-absolute top-50 start-50 translate-middle">
                          <div class="mb-3">
                              <label for="nameBox" class="form-label">Name</label>
                              <input type="text" class="form-control" id="nameBox" aria-describedby="emailHelp">
                          </div>
                          <div class="mb-3">
                              <label for="emailBox" class="form-label">Your Email Address</label>
                              <input type="email" class="form-control" id="emailBox">
                          </div>
                          <div class="mb-3">
                              <label for="subjectBox" class="form-label">Subject</label>
                              <input type="text" class="form-control" id="subjectBox">
                          </div>
                          <div class="mb-3">
                              <label for="bodyBox" class="form-label">Body</label>
                              <textarea class="form-control" id="bodyBox" rows="3"></textarea>
                            </div>
                          <button type="submit" class="btn btn-primary">Send Email</button>
                      </form> 
                  </div>
                  <div style="z-index: 1;" class="col col-6">
                      <img style="opacity: 0%;" class="img-fluid" src="/Profile_Pic.JPG" alt="Picture of Frank" />
                  </div>
              </div>
          </div>
    </div>

   



    

    <script src="/publicJS/main.js"></script>

  </body>
</html>